{
  "kind": {
    "group": "networking.k8s.io",
    "kind": "Ingress",
    "version": "v1"
  },
  "object": {
    "apiVersion": "networking.k8s.io/v1",
    "kind": "Ingress",
    "metadata": {
      "name": "partial-tls"
    },
    "spec": {
      "tls": [
        {
          "hosts": [
            "foo.bar.co"
          ],
          "secretName": "testsecret-tls"
        }
      ],
      "rules": [
        {
          "host": "foo.bar.com",
          "http": {
            "paths": [
              {
                "pathType": "Prefix",
                "path": "/bar",
                "backend": {
                  "service": {
                    "name": "service1",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        },
        {
          "host": "*.foo.com",
          "http": {
            "paths": [
              {
                "pathType": "Prefix",
                "path": "/foo",
                "backend": {
                  "service": {
                    "name": "service2",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    }
  }
}
