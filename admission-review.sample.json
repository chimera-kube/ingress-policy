{
  "apiVersion": "admission.k8s.io/v1beta1",
  "kind": "AdmissionReview",
  "request": {
    "kind": {
      "group": "extensions",
      "kind": "Ingress",
      "version": "v1beta1"
    },
    "object": {
      "metadata": {
        "name": "prod"
      },
      "spec": {
        "tls": [
          {
            "hosts": ["some-host.com"],
            "secretName": "secret-name-tls"
          }
        ],
        "rules": [
          {
            "host": "some-host.com",
            "http": {
              "paths": [
                {
                  "path": "/",
                  "backend": {
                    "service": {
                      "name": "service",
                      "port": 443
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  }
}
